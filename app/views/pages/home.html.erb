<h1>Banner goes here</h1>

<h2>My lists</h2>

<ul>
  <% @lists.each do |list| %>
    <li>
      <%= link_to "#{list.name}", list_path(list) %>
      <%= cl_image_tag list.photo.key, height: 40, width: 40, crop: :fill, radius: 50 if list.photo.attached? %>
    </li>
  <% end %>
</ul>

<%= link_to "Add a new list", new_list_path%>
